<br/><br/>
<%= form_for(@search_analytic) do |f| %>
  <div class="row padding-top-20">
    <div class="col-xs-10">
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="col-xs-10">
            <%= f.text_field :article_title, placeholder: t(".search"), class: "form-control", onkeyup: "save_search()" %>
          </div>
          <div class="col-xs-2">
            <%= link_to t(".show_analytics"), show_search_analytics_path, class: "btn btn-success" %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <%= image_tag "ajax-loader.gif", class:"loading" %>
  <div id="results"></div>
<% end %>
