<br/><br/>
<%= link_to t(".back"), search_analytics_path, class: "btn btn-info" %>
<%= link_to t(".delete"), search_analytic_path(0), method: :delete,
            data: { confirm: t(".delete_confirm")}, class: "btn btn-danger"  %>
<br/><br/>
<div class="row">
  <div class="col-xs-12">
    <% if @search_analytics.empty? %>
      <%= t(".nothing_to_show") %>
    <% else %>
      <table class="table table-striped">
        <tr>
          <th><%= t(".article_title") %></th>
          <th><%= t(".frequency") %></th>
        </tr>
        <% @search_analytics.each do |search| %>
          <tr>
            <td><%= search.article_title %></td>
            <td><%= search.quantity %></td>
          </tr>
        <% end %>
      </table>
    <% end %>
  </div>
</div>
